[DEFAULT]
player = mpv --no-resume-playback
verbose = false
engine = acestreamplayer.engine --client-console

[tty]
player = mpv --no-resume-playback
verbose = true
engine = acestreamplayer.engine --client-console

[browser]
player = mpv --no-resume-playback
engine = acestreamplayer.engine --client-gtk
verbose = false

